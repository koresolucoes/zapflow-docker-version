{
  "name": "zapflow-api",
  "version": "1.0.0",
  "description": "API for Zapflow application",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "node --import tsx watch server.ts",
    "worker:campaign": "tsx ./workers/startWorker.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker:campaign\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@google/genai": "^1.11.0",
    "@supabase/supabase-js": "^2.53.0",
    "bullmq": "^5.56.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "redis": "^4.6.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.17.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  }
}
