# URL base da aplicação (ex: http://localhost:5173, https://seu-dominio.com.br)
VITE_APP_URL=http://localhost:5173

# Configurações do Supabase
VITE_SUPABASE_URL=seu_url_do_supabase
VITE_SUPABASE_ANON_KEY=sua_chave_anonima_do_supabase

# Chave da API do Google Gemini (opcional)
VITE_GEMINI_API_KEY=sua_chave_da_api_gemini

# Configurações de E-mail (opcional)
SMTP_HOST=smtp.exemplo.com
SMTP_PORT=587
SMTP_USER=seu_usuario
SMTP_PASS=sua_senha
SMTP_FROM=noreply@seu-dominio.com

# Configurações de Autenticação (opcional)
JWT_SECRET=sua_chave_secreta_jwt

# Configurações de Banco de Dados (opcional)
DATABASE_URL=sua_url_de_conexao_com_banco_de_dados
